{
  "name": "vsc-terminal-buttons",
  "displayName": "Rename terminal Button",
  "description": "Adds a button to the right hand side of the statusbar to run the 'Rename terminal' command",
  "version": "1.0.2",
  "activationEvents": [
    "onStartupFinished"
  ],
  "author": {
    "name": "sketchbuch"
  },
  "bugs": {
    "url": "https://github.com/sketchbuch/vsc-terminal-buttons/issues"
  },
  "categories": [
    "Other"
  ],
  "engines": {
    "vscode": "^1.85.0"
  },
  "icon": "docs/images/icon/icon.png",
  "keywords": [
    "statusbar",
    "terminal"
  ],
  "license": "SEE LICENSE IN LICENSE.txt",
  "publisher": "sketchbuch",
  "main": "./out/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/sketchbuch/vsc-terminal-buttons"
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "yarn run compile && node ./node_modules/vscode/bin/test"
  },
  "dependencies": {
    "vscode-ext-localisation": "^1.1.0"
  },
  "devDependencies": {
    "@arkweid/lefthook": "^0.7.7",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/sinon": "^9.0.9",
    "@types/vscode": "^1.50.0",
    "chai": "^4.2.0",
    "commitlint": "^8.3.5",
    "lefthook": "^1.8.2",
    "sinon": "^9.2.2",
    "ts-mockito": "^2.3.1",
    "typescript": "^4.9.3",
    "vscode-test": "^1.3.0"
  }
}
